}

/*												*/

QDialog {
    border: 1px solid rgb(247, 188, 128);
    border-radius: 5px;
	background: rgb(255, 225, 186);
}

/*									*/

QPushButton#createResetBtn, QPushButton#searchResetButton {
    background-color: qradialgradient(cx:0, cy:0, radius: 1.6, fx:0.5, fy:0.5, 
		stop:0  rgb(255, 239, 229), stop:0.8 rgb(255, 103, 86), stop:1  rgb(255, 103, 86));
	border-radius: 5px;
    border: 1px solid #8f8f91;
}

QPushButton#createSaveBtn{
    border: 1px solid #8f8f91;
    border-radius: 5px;
    background-color: qradialgradient(cx:0, cy:0, radius: 1.2, fx:0.5, fy:0.5, 
		stop:0  rgb(232, 255, 209), stop:0.8 rgb(186, 242, 130), stop:1  rgb(186, 242, 130));
}

QPushButton {
	background-color: rgb(247, 188, 128);
}


QPushButton:pressed {
    background-color: qradialgradient(cx:0, cy:0, radius: 1.6, fx:0.5, fy:0.5, 
		stop:0  rgb(255, 239, 229), stop:0.8 rgb(247, 188, 128), stop:1  rgb(247, 188, 128));
}

QPushButton:flat {
    border: none; /* no border for a flat push button */
}

QPushButton:default {
    border-color: navy; /* make the default button prominent */
}

/*												*/

QComboBox {
	border: 1px solid grey;
    border-radius: 5px;
	background: black;
}

QComboBox:!editable, QComboBox::drop-down:editable {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 rgb(255, 145, 35), stop: 0.2 rgb(247, 188, 128),
                                 stop: 0.8  rgb(247, 188, 128), stop: 1.0 rgb(255, 145, 35));
}

/* QComboBox gets the &quot;on&quot; state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
     background: rgb(247, 188, 128);
}

QComboBox:on { /* shift the text when the popup opens */
    padding-top: 3px;
    padding-left: 4px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;

    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {	
    image: url(:/img/arrow_down.png);
	width: 10px;
	height: 10px;
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
    top: 1px;
    left: 1px;
}

/*														*/

QScrollArea {
	background-color: transparent;
}

QWidget#searchIngredientScrollAreaContents, QWidget#searchAddedIngredientScrollAreaContents, QWidget#searchKeywordScrollAreaContents, QWidget#searchAddedKeywordScrollAreaContents  {
	background-color:  rgb(255, 225, 186);
